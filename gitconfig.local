[alias]
  a = add
  an = add -N
  b = branch
  br = branch --remote
  ca = commit --amend
  cl = clone
  cob = checkout -b
  com = checkout master
  d = diff
  dm = diff master
  ds = diff --staged
  l = log --pretty=format:'%C(yellow)%h %C(red)%ad %C(blue)%an %C(green)%d %C(reset)%s' --date=short
